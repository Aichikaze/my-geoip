{
  "input": [
    {
      "type": "v2rayGeoIPDat",
      "action": "add",
      "args": {
        "uri": "https://raw.githubusercontent.com/Loyalsoldier/geoip/release/geoip.dat"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "direct",
        "uri": "https://raw.githubusercontent.com/hydraponique/roscomvpn-geoip/release/direct.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "akamai",
        "uri": "https://techdocs.akamai.com/property-manager/pdfs/akamai_ipv4_CIDRs.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "akamai",
        "uri": "https://techdocs.akamai.com/property-manager/pdfs/akamai_ipv6_CIDRs.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "discord",
        "uri": "https://raw.githubusercontent.com/itdoginfo/allow-domains/main/Subnets/IPv4/discord.lst"
      }
    },
    {
      "type": "json",
      "action": "add",
      "args": {
        "name": "gcore",
        "uri": "https://api.gcore.com/cdn/public-ip-list",
        "jsonPath": ["addresses", "addresses_v6"]
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "oracle",
        "uri": "https://raw.githubusercontent.com/Davoyan/ipinfo/main/CDN/oracle.lst"
      }
    },
    {
      "type": "maxmindGeoLite2ASNCSV",
      "action": "add",
      "args": {
        "ipv4": "https://raw.githubusercontent.com/Loyalsoldier/geoip/release/GeoLite2-ASN-Blocks-IPv4.csv",
        "ipv6": "https://raw.githubusercontent.com/Loyalsoldier/geoip/release/GeoLite2-ASN-Blocks-IPv6.csv",
        "wantedList": {
          "akamai": ["AS63949"],
          "cdn77": ["AS60068"],
          "digitalocean": ["AS14061"],
          "hetzner": ["AS24940"],
          "ovh": ["AS16276"],
          "scaleway": ["AS12876"],
          "vultr": ["AS20473"]
        }
      }
    }
  ],
  "output": [
    {
      "type": "v2rayGeoIPDat",
      "action": "output",
      "args": {
        "outputDir": "./output",
        "outputName": "geoip.dat",
        "wantedList": ["direct", "akamai", "cdn77", "cloudflare", "cloudfront", "digitalocean", "discord", "fastly", "gcore", "telegram", "hetzner", "ovh", "oracle", "vultr", "scaleway"]
      }
    },
    {
      "type": "maxmindMMDB",
      "action": "output",
      "args": {
        "outputDir": "./output",
        "outputName": "Country.mmdb",
        "overwriteList": ["direct", "akamai", "cdn77", "cloudflare", "cloudfront", "digitalocean", "discord", "fastly", "gcore", "telegram", "hetzner", "ovh", "oracle", "vultr", "scaleway"]
      }
    },
    {
      "type": "singboxSRS",
      "action": "output"
    },
    {
      "type": "mihomoMRS",
      "action": "output"
    },
    {
      "type": "text",
      "action": "output"
    },
    {
      "type": "clashRuleSetClassical",
      "action": "output"
    },
    {
      "type": "surgeRuleSet",
      "action": "output",
      "args": {
        "addSuffixInLine": ",no-resolve"
      }
    }
  ]
}
